{
    "name": "tbcd/file-archiver",
    "type": "library",
    "description": "A library to manage files archiving as well as their rotation",
    "authors": [
        {
            "name": "Thomas Beauchataud"
        }
    ],
    "license": "MIT",
    "keywords": [
        "file",
        "symfony",
        "archive",
        "archiver",
        "zip"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "2.0.0",
    "require": {
        "php": ">=8.1",
        "ext-zip": "*",
        "symfony/filesystem": "^6.0",
        "league/flysystem": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\FileArchiver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBCD\\Tests\\FileArchiver\\": "tests/"
        }
    }
}
